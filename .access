people/*splat:
  rewrite: /person

data/profiles.json:
  collection: true

data/photos/:user:
  collection: true

data/photos/:user/*splat:
  write: $user
  delete: $user

data/images/:user/*splat:
  write: $user
  delete: $user

data/profiles/:user.json:
  write: $user
  delete: $user

data/follows/:user/follows.json:
  write: $user
  delete: $user
